package index;
import java.util.*;
public class ind {
	public static void main(String args[])
	{
		String s = "abcdcasdfa";
		for(int i = 65;i<123;i++){
			int count = 0;
			for(int j = 0;j<s.length();j++)
			{
				if(i==s.charAt(j)){
					count++;
				}
			}
			if(count != 0){
				System.out.println((char)i+":出现了"+count+"次");
			}
		}
	}
}
